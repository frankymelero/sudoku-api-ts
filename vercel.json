{
    "version": 2,
    "name": "socketing",
    "builds": [
      {
        "src": "dist/app.js",  
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
          "src": "/api/v1/(.*)",  
          "dest": "/dist/app.ts"  
        },
        {
          "src": "/(.*)",  
          "dest": "/dist/app.ts"  
        }
      ]
    }
  